class Solution{
    public:
    Node* deleteMid(Node* head)
    {
        Node *slow=head;
        Node *fast=head;
        fast=fast->next->next; 
        
        while(fast!=NULL && fast->next!=NULL){
            fast=fast->next->next;
            slow=slow->next;
        }
        
        slow->next=slow->next->next;
        
        return head;
    }
};
