class Solution {
  public:
    vector<int> constructList(int q, vector<vector<int>> &queries) {
        int xors = 0;
        vector<int> ans;
        
        for(int i=q-1; i>=0; i--){
            if(queries[i][0] == 0)
                ans.push_back(queries[i][1] ^ xors);    
            else
                xors ^= queries[i][1];
        }
        
        ans.push_back(xors);
        sort(ans.begin(), ans.end());
        
        return ans;
    }
};
